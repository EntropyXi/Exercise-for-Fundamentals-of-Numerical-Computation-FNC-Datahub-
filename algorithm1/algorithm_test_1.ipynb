   "source": [
    "\n",
    "def poly1(p, x):\n",
    "    y = 0\n",
    "    n = len(p)\n",
    "    for i in range(n):\n",
    "        y = y + p[i] * x**i\n",
    "    return y\n",
    "\n",
    "a = [0, -1, 2]\n",
    "poly1(a, 1)\n",
