    "import math\n",
    "\n",
    "def samplevar(x):\n",
    "    y = 0\n",
    "    i = 0\n",
    "    n = len(x)\n",
    "    for i in range(n):\n",
    "        i += x[i]\n",
    "    average = i / n\n",
    "    for j in range(n):\n",
    "        y += math.fabs((x[j] - average)**2)\n",
    "    var = y / n\n",
    "    return n \n",
    "\n",
    "p = [1, 1, 1]\n",
    "samplevar(p)"
